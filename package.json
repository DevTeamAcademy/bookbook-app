  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "compat"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write --print-width 120 --single-quote --jsx-single-quote --trailing-comma all",
      "git add"
    ]
  },
  "browserslist": {
    "development": [
      "last 2 versions"
    ],
    "production": [
      "last 4 versions"
    ]
